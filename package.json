{
	"name": "vscode-shex-extension",
	"description": "ShEx extension for Visual Studio Code",
	"version": "0.0.1",
	"publisher": "PabloMenndez",
	"engines": {
		"vscode": "^1.38.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:shex.start",
		"onCommand:turtle.start",
		"onCommand:shapeMap.start",
		"onCommand:validation.start",
		"onWebviewPanel:shex"
	],
	"repository": {
		"type": "git",
		"url": "https://github.com/mistermboy/vscode-shex-extension"
	},
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "shex.start",
				"title": "ShEx Editor",
				"category": "ShEx"
			},
			{
				"command": "turtle.start",
				"title": "Turtle Editor",
				"category": "ShEx"
			},
			{
				"command": "shapeMap.start",
				"title": "ShapeMap Editor",
				"category": "ShEx"
			},
			{
				"command": "validation.start",
				"title": "Validation",
				"category": "ShEx"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"lint": "tslint -p ./",
		"watch": "tsc -w -p ./"
	},
	"dependencies": {},
	"devDependencies": {
		"@types/node": "^12.12.0",
		"tslint": "^5.19.0",
		"typescript": "^3.8.3",
		"@types/vscode": "^1.38.0"
	}
}
