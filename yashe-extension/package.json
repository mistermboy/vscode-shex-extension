{
	"name": "yashe",
	"description": "Yet Another ShEx Editor",
	"version": "0.0.4",
	"publisher": "PabloMenndez",
	"engines": {
		"vscode": "^1.38.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:yashe.start",
		"onWebviewPanel:yashe"
	],
	"repository": {
		"type": "git",
		"url": "https://github.com/mistermboy/vscode-shex-extensions"
	},
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "yashe.start",
				"title": "Start YASHE Editor",
				"category": "ShEx"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"lint": "tslint -p ./",
		"watch": "tsc -w -p ./"
	},
	"dependencies": {},
	"devDependencies": {
		"@types/node": "^12.12.0",
		"tslint": "^5.19.0",
		"typescript": "^3.8.3",
		"@types/vscode": "^1.38.0"
	}
}
