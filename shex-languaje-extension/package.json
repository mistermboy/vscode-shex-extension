{
    "name": "shex",
    "displayName": "shex",
    "description": "shex extension",
    "version": "0.0.4",
    "publisher": "PabloMenndez", 
    "engines": {
        "vscode": "^1.44.0"
    },
    "repository": {
		"type": "git",
		"url": "https://github.com/mistermboy/vscode-shex-extensions"
	},
    "categories": [
        "Programming Languages"
    ],
    "contributes": {
        "languages": [{
            "id": "shex",
            "aliases": ["ShEx (Shape Expressions)", "shex"],
            "extensions": [".shex"],
            "configuration": "./language-configuration.json"
        }],
        "grammars": [{
            "language": "shex",
            "scopeName": "shexDoc",
            "path": "./syntaxes/shex.tmLanguage.json"
        }],
        "snippets": [
            {
              "language": "shex",
              "path": "./snippets/shex.json"
            }
          ]
    }
}