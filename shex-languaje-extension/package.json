{
    "name": "shex",
    "displayName": "shex",
    "description": "ShEx extension",
    "version": "0.0.1",
    "publisher": "PabloMenndez",
    "engines": {
        "vscode": "^1.44.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "repository": {
		"type": "git",
